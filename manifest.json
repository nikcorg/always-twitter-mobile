{
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "description": "Ensure all visits to Twitter are to mobile.twitter.com",
  "homepage_url": "https://github.com/nikcorg/always-twitter-mobile",
  "manifest_version": 2,
  "name": "Always Twitter Mobile",
  "permissions": [
    "*://www.twitter.com/*",
    "*://twitter.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.9"
}
